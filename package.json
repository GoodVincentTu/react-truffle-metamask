{
  "name": "react-truffle-metamask",
  "version": "0.1.0",
  "author": "PhyrexTsai",
  "engines": {
    "node": "8.x.x",
    "npm": "5.x.x",
    "yarn": "1.x.x"
  },
  "dependencies": {
    "lint-staged": "^5.0.0",
    "material-ui": "^1.0.0-beta.6",
    "material-ui-icons": "^1.0.0-beta.5",
    "moment": "^2.19.2",
    "node-sass-chokidar": "^0.0.3",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.8.2",
    "raf": "^3.4.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "truffle-hdwallet-provider": "0.0.3",
    "typeface-roboto": "0.0.45",
    "web3": "^0.20.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src -o src",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src -o src --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom --setupTestFrameworkScriptFile=raf/polyfill",
    "eject": "react-scripts eject",
    "dev:rmtestbuild": "rm -rf testbuild",
    "dev:buildtestlib": "babel src/lib -d testbuild && babel test -d testbuild",
    "predev": "npm run dev:rmtestbuild && npm run dev:buildtestlib",
    "dev": "node testbuild/dAppService.test.js",
    "postdev": "npm run dev:rmtestbuild"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1"
  }
}
